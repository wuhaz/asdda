	Name = "Discord",
	Content = "Discord Invite Copied To Clipboard",
	Image = "rbxassetid://4483345998",
	Time = 5
})
end)
local Combat = MainUI.AddPage("Combat")
local Aim = Combat.AddLabel("Aim")
local Aimbot = Combat.AddToggle("Aimbot", false, function(V)
    if V == true then
        local Camera = workspace.CurrentCamera
        local Players = game:GetService("Players")
        local RunService = game:GetService("RunService")
        local TweenService = game:GetService("TweenService")
        local LocalPlayer = Players.LocalPlayer

        _G.AimbotEnabled = true
        _G.TeamCheck = true
        _G.AimPart = "Head"
        _G.Sensitivity = 0 -- Adjust this value according to how fast you want the aimbot to lock onto targets

        local function GetClosestPlayer()
            local MaximumDistance = math.huge
            local Target = nil

            for _, v in ipairs(Players:GetPlayers()) do
                if v ~= LocalPlayer and (not _G.TeamCheck or v.Team ~= LocalPlayer.Team) and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health > 0 then
                    local CameraPosition = Camera.CFrame.Position
                    local PlayerPosition = v.Character.HumanoidRootPart.Position
                    local VectorDistance = (CameraPosition - PlayerPosition).Magnitude

                    if VectorDistance < MaximumDistance then
                        Target = v
                        MaximumDistance = VectorDistance
                    end
                end
            end

            return Target
        end

        RunService.RenderStepped:Connect(function()
            if _G.AimbotEnabled then
                local closestPlayer = GetClosestPlayer()
                if closestPlayer then
                    local targetPosition = closestPlayer.Character[_G.AimPart].Position
                    TweenService:Create(Camera, TweenInfo.new(_G.Sensitivity, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {CFrame = CFrame.new(Camera.CFrame.Position, targetPosition)}):Play()
                end
            end
        end)
    elseif V == false then
        _G.AimbotEnabled = false
    end
end)
local Silent Aim = Combat.AddButton("Silent Aim", function()
loadstring(game:HttpGet("https://pastebin.com/raw/KpQhjvRQ",true))()
end)
local Gun = Combat.AddLabel("Gun Mods")
local Ammo = Combat.AddToggle("Infinite Ammo", false, function(V)
    if V == true then
        _G.InfiniteAmmoEnabled = true
        
        while _G.InfiniteAmmoEnabled do
            game:GetService("Players").LocalPlayer.PlayerGui.GUI.Client.Variables.ammocount.Value = 999
            game:GetService("Players").LocalPlayer.PlayerGui.GUI.Client.Variables.ammocount2.Value = 999
            wait()
        end
    elseif V == false then
        _G.InfiniteAmmoEnabled = false
    end
end)
local Gun Mods = Combat.AddButton("Gun Mods", function()
local replicationstorage = game.ReplicatedStorage

for i, v in pairs(replicationstorage.Weapons:GetDescendants()) do
   if v.Name == "Auto" then
       v.Value = true
   end
   if v.Name == "RecoilControl" then
       v.Value = 0
   end
   if v.Name == "MaxSpread" then
       v.Value = 0
   end
   if v.Name == "ReloadTime" then
      v.Value = 1
   end
   if v.Name == "FireRate" then
       v.Value = 0.05
   end
   if v.Name == "Crit" then
       v.Value = 20
   end
end
end)
