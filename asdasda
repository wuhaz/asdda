local MainUI = UILibrary.Load("MySploit")
local FirstPage = MainUI.AddPage("Home")

local Welcome = FirstPage.AddLabel("Welcome To MySploit")
local FirstButton = FirstPage.AddButton("Game: Arsenal", function()
print("Hi")
end)
local Discord = FirstPage.AddButton("Copy Discord Invite", function()
     		setclipboard("https://discord.gg/6aTPcbvwT5")
